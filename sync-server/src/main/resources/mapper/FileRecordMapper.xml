<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yumumu.syncServer.mapper.FileRecordMapper">

    <resultMap id="BaseResultMap" type="com.yumumu.syncServer.model.po.FileRecord">
        <id property="id" column="ID" jdbcType="OTHER"/>
        <result property="filename" column="FILENAME" jdbcType="VARCHAR"/>
        <result property="fileSize" column="FILE_SIZE" jdbcType="OTHER"/>
        <result property="md5" column="MD5" jdbcType="VARCHAR"/>
        <result property="version" column="VERSION" jdbcType="OTHER"/>
        <result property="createTime" column="CREATE_TIME" jdbcType="TIMESTAMP"/>
        <result property="enable" column="ENABLE" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID
        ,FILENAME,FILE_SIZE,
        MD5,VERSION,CREATE_TIME,
        ENABLE
    </sql>
</mapper>
